{
    "name": "oslab/slim-monolog",
    "description": "Provide Logger on Slim application.",
    "type": "library",
    "keywords": ["slim", "logger", "monolog"],
    "license": "MIT",
    "authors": [
        {
            "name": "florent",
            "email": "orions07@gmail.com"
        }
    ],
    "require": {
        "monolog/monolog": "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "squizlabs/php_codesniffer": "~2.8"
    },
    "autoload": {
        "psr-4": {
            "OsLab\\Slim\\": ""
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "cs": "./vendor/bin/phpcs --standard=PSR2  -n -p src/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
